<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">

      <record model="ir.module.category" id="product_management">
           <field name="name">Prueba</field>
           <field name="description">User access level for Product module</field>
      </record>

      <record id="prueba_user" model="res.groups">
          <field name="name">Usuario</field>
          <field name="category_id" ref="product_management"/>
      </record>

      <record id="prueba_admin" model="res.groups">
          <field name="name">Administrador</field>
          <field name="category_id" ref="product_management"/>
          <field name="implied_ids" eval="[(4, ref('prueba_user'))]"/>
      </record>

  </data>
</odoo>
